{
	"name": "listspink-frontend",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "PUBLIC_DEV=true vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"test": "npm run test:unit:ci && npm run test:e2e",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint . && npm run check",
		"format": "prettier --write .",
		"test:e2e": "playwright test",
		"test:unit:ci": "vitest run",
		"test:unit": "vitest",
		"check:all": "npm run check && npm run lint && npm run test:unit -- --run",
		"deploy": "npx vercel",
		"deploy-prod": "npx vercel --prod"
	},
	"devDependencies": {
		"@playwright/test": "1.39.0",
		"@sveltejs/adapter-auto": "2.1.0",
		"@sveltejs/adapter-vercel": "3.0.3",
		"@sveltejs/kit": "1.27.2",
		"@tailwindcss/forms": "0.5.6",
		"@types/uuid": "9.0.4",
		"@typescript-eslint/eslint-plugin": "5.62.0",
		"@typescript-eslint/parser": "5.62.0",
		"autoprefixer": "10.4.16",
		"postcss": "8.4.31",
		"svelte": "4.2.2",
		"svelte-check": "3.5.2",
		"tailwindcss": "3.3.5",
		"tslib": "2.6.2",
		"typescript": "^5.0.0",
		"vercel": "32.5.0",
		"vite": "^4.3.6",
		"vite-plugin-top-level-await": "1.3.1",
		"vitest": "^0.34.0"
	},
	"type": "module",
	"dependencies": {
		"@auth0/auth0-spa-js": "2.1.2",
		"@clerk/clerk-js": "4.64.1",
		"svelte-french-toast": "1.2.0",
		"uuid": "9.0.1",
		"zod": "3.22.4"
	}
}

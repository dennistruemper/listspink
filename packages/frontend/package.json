{
	"name": "listspink-frontend",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "PUBLIC_DEV=true vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"test": "npm run test:unit:ci && npm run test:e2e",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint . && npm run check",
		"format": "prettier --write .",
		"test:e2e": "playwright test",
		"test:unit:ci": "vitest run",
		"test:unit": "vitest",
		"check:all": "npm run check && npm run lint && npm run test:unit -- --run",
		"deploy": "npx vercel",
		"deploy-prod": "npx vercel --prod"
	},
	"devDependencies": {
		"@playwright/test": "^1.28.1",
		"@sveltejs/adapter-auto": "^2.0.0",
		"@sveltejs/adapter-vercel": "^3.0.0",
		"@sveltejs/kit": "^1.20.4",
		"@tailwindcss/forms": "0.5.6",
		"@types/uuid": "9.0.3",
		"@typescript-eslint/eslint-plugin": "^5.45.0",
		"@typescript-eslint/parser": "^5.45.0",
		"autoprefixer": "10.4.15",
		"postcss": "8.4.29",
		"svelte": "^4.0.0",
		"svelte-check": "^3.4.3",
		"tailwindcss": "3.3.3",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vercel": "32.1.0",
		"vite": "^4.3.6",
		"vite-plugin-top-level-await": "1.3.1",
		"vitest": "^0.34.0"
	},
	"type": "module",
	"dependencies": {
		"@auth0/auth0-spa-js": "2.1.2",
		"@clerk/clerk-js": "4.56.2",
		"svelte-french-toast": "1.2.0",
		"uuid": "9.0.0",
		"zod": "3.22.2"
	}
}

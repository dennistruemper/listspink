{
	"name": "listspink",
	"version": "0.0.5",
	"description": "",
	"private": true,
	"main": "packages/backend/index.ts",
	"type": "module",
	"scripts": {
		"dev-be": "ampt --skip-update",
		"dev-fe": "export BACKEND_URL=$(./scripts/getSandboxUrl.sh) && cd packages/frontend && echo PUBLIC_BACKEND_URL $BACKEND_URL && PUBLIC_BACKEND_URL=$BACKEND_URL npm run dev",
		"dev-test": "npx ampt run test-all-watch --test-stage",
		"lint": "cd packages/frontend && npm run lint && cd -",
		"test-local": "BASE_PATH=** MODE=LOCAL vitest run",
		"format": "prettier --write .",
		"clean": "git checkout main && git pull && npm ci",
		"new-branch": "npm run clean && git checkout -b $1",
		"rebase": "git stash && git checkout main && git pull && git checkout - && git rebase main && git stash apply",
		"ampt:test": "BASE_PATH=packages/backend MODE=SANDBOX vitest run",
		"ampt:test-all": "BASE_PATH=packages/backend MODE=ALL vitest run",
		"ampt:test-all-watch": "BASE_PATH=packages/backend MODE=ALL vitest"
	},
	"dependencies": {
		"@ampt/data": "1.0.3",
		"@ampt/sdk": "1.0.3",
		"@clerk/clerk-sdk-node": "4.12.13",
		"cors": "2.8.5",
		"express": "4.18.2",
		"express-oauth2-jwt-bearer": "1.6.0",
		"ksuid": "3.0.0",
		"zod": "3.22.4"
	},
	"ampt": {
		"app": "lists-pink-backend",
		"org": "dennistruemper269889"
	},
	"devDependencies": {
		"@ampt/cli": "1.0.11",
		"@types/express": "4.17.18",
		"@types/node": "20.8.0",
		"@types/supertest": "2.0.13",
		"eslint": "8.49.0",
		"eslint-config-prettier": "8.10.0",
		"eslint-plugin-svelte": "2.33.1",
		"jsonwebtoken": "9.0.2",
		"prettier": "3.0.3",
		"prettier-plugin-svelte": "3.0.3",
		"supertest": "6.3.3",
		"vite": "4.4.11",
		"vitest": "0.34.6"
	},
	"workspaces": [
		"packages/*"
	],
	"files": [
		"*"
	]
}
